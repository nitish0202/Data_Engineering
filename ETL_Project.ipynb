   "source": [
    "#Change Names of some countries to maintain the consistency of data that we get from different sources \n",
    "\n",
    "data = data.apply(lambda x:x.replace(\"Congo (Kinshasa)\", \"DR Congo\"))\n",
    "data = data.apply(lambda x:x.replace(\"Congo (Brazzaville)\", \"Congo\"))\n",
    "data = data.apply(lambda x:x.replace(\"Ivory Coast\", \"Côte d'Ivoire\"))"
   ]
  },
