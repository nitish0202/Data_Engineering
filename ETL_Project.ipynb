   "source": [
    "#Get population data for each country from Rapid API\n",
    "# API link - https://world-population.p.rapidapi.com/population\n",
    "\n",
    "api_url = \"https://world-population.p.rapidapi.com/population\"\n",
    "\n",
    "#Headers can be found on respective websites\n",
    "headers = {\n",
    "    'x-rapidapi-host': \"world-population.p.rapidapi.com\",\n",
    "    'x-rapidapi-key': \"******************************************"\n",
    "    }\n",
    "\n",
    "#Add column to the dataframe\n",
    "data[\"Population\"] = np.nan\n",
    "\n",
    "#loop over countries and get population and fill it in the dataframe\n",
    "for country in tqdm(data[\"Country or region\"].to_list()):\n",
    "    # create querystring for API call\n",
    "    query = {\"country_name\":country}\n",
    "    \n",
    "    response = requests.request(\"GET\",api_url,headers=headers,params=query)\n",
    "    \n",
    "    r_dict = json.loads(response.text)\n",
    "    if response.status_code == 200:\n",
    "        response_dict = json.loads(response.text)\n",
    "    if response_dict[\"ok\"] == True:\n",
    "        population = response_dict[\"body\"][\"population\"]\n",
    "        data.loc[data[\"Country or region\"] == country, \"Population\"] = population"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "a116b556",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Get the data of average age and median age of each country \n",
    "avg_resp = requests.get(\"https://www.worlddata.info/average-age.php\")\n",
    "\n",
    "soup_avg = BeautifulSoup(avg_resp.text, 'html.parser')\n",
    "\n",
    "table_avg = soup_avg.find_all('table', {'class':'std100 hover'})\n",
    "\n",
    "table_med = table_avg[0]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "5a607dda",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Coverting the html data to a dataframe\n",
    "df_1 = pd.read_html(str(table_med))\n",
    "\n",
    "df_med = pd.DataFrame(df_1[0])\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "id": "4a9a5329",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Joining both the dataframes on country to get final data of each country\n",
    "df_final = data.set_index(\"Country or region\").join(df_med.set_index(\"Country\")).reset_index()\n"
   ]
  },
